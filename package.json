{
  "name": "shapermintcypress",
  "version": "1.0.0",
  "description": "automation",
  "main": "index.js",
  "dependencies": {
    "cypress": "^4.2.0"
  },
  "devDependencies": {
    "cypress-cucumber-preprocessor": "^2.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "multiple-cucumber-html-reporter": "^1.16.0"
  },
  "scripts": {
    "start": "npx cypress open",
    "cy:run": "node ./cypress/support/utils/cypressRunner.js",
    "cy:report": "node ./cypress/support/utils/reporter.js",
    "test": "npm run cy:run && npm run cy:report"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/reports",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  },
  "author": "Sophie Carreras",
  "license": "ISC"
}
